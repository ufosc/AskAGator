<!DOCTYPE html>
<html>
  <head>
    <title>Myapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="alert alert-warning">
      <%= flash[:alert] %>
    </div>
    <div class="alert alert-primary">
      <%= flash[:notice] %>
    </div>
    <%= yield %>
  </body>
</html>
